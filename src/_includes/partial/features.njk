{% set alt = '' %}
{% if collections.service.length % 2 == 1 %}
  {% set alt = 'alt' %}
{% endif %}
<section class="wrapper {{ alt }}">
  <div class="inner">
    <h2 class="major">{{ config.features.header }}</h2>
    <p>{{ config.features.subheader }}</p>
    <section class="features">
      {% for feature in collections.feature | ordered %}
        {% include 'partial/feature.njk' %}
      {% endfor %}
    </section>
    {# <ul class="actions">
      <li>
        <a href="#" class="button">Browse All</a>
      </li>
    </ul> #}
  </div>
</section>
