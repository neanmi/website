{
  "name": "main-website",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public",
    "minify:js:main": "minify src/assets/js/main.js > src/assets/js/main.min.js",
    "minify:js:util": "minify src/assets/js/util.js > src/assets/js/util.min.js",
    "minify:js": "run-p minify:js:main minify:js:util",
    "minify:css:main": "minify src/assets/css/main.css > src/assets/css/main.min.css",
    "minify:css:noscript": "minify src/assets/css/noscript.css > src/assets/css/noscript.min.css",
    "minify:css": "run-p minify:css:main minify:css:noscript",
    "sass-compile": "sass src/assets/sass/main.scss src/assets/css/main.css",
    "css-compile": "run-s sass-compile minify:css",
    "start:eleventy": "cross-env NODE_ENV=development eleventy --serve",
    "start:netlify-cms-proxy": "netlify-cms-proxy-server",
    "start:concurrently": "concurrently --kill-others \"npm run start:eleventy\" \"npm run start:netlify-cms-proxy\"",
    "start": "run-s clean start:concurrently",
    "prebuild": "run-s clean css-compile minify:js",
    "build": "cross-env NODE_ENV=production eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "2.0.0",
    "concurrently": "7.6.0",
    "cross-env": "7.0.3",
    "follow-redirects": "1.15.2",
    "luxon": "3.3.0",
    "markdown-it": "13.0.1",
    "minify": "9.2.0",
    "netlify-cms-proxy-server": "1.3.24",
    "npm-run-all": "4.1.5",
    "nunjucks": "3.2.3",
    "rimraf": "4.4.1",
    "sass": "1.60.0"
  }
}
